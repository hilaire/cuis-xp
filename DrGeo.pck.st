!DrGPointMorph methodsFor: 'drawing' stamp: 'hlsf 8/5/2021 14:54:24'!
drawOn: aCanvas
| shape center |
	self isDrawNeeded ifFalse: [^ self].
	self updateLabelStatus.
	shape _ style shape.
	center _ self morphCenter.
	shape == #square ifTrue:	 	[
		aCanvas 
			fillRectangle: (Rectangle center: center extent: style pointExtent)
			color: color.
		^ self].
	shape == #round ifTrue:		[ | extent |
		extent _ style pointExtent / 2.0.
		aCanvas ellipseCenterX: center x  y: center y rx: extent x ry: extent y 
			borderWidth: 0 borderColor: nil fillColor: color.
		^ self].
	shape == #cross ifTrue:[ | e |
		e _ style pointExtent / 2.0.
		aCanvas line: center - e to: center + e width: 1 color: color.
		aCanvas line: center + (e * (-1@1)) to: center + (e * (1@-1)) width: 1 color: color]! !

