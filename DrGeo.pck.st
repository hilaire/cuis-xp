	(parents
			noneSatisfy: [:item | item isValueItem or: [item isSegmentItem]])
		ifTrue: [^ super moveParent: aPoint].
	"we just want to move the center point, not the radius value"
	thePoint _ parents first.
	thePoint moveEvent: aPoint.
	^ true! !

