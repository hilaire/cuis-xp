!DrGeoPresenter methodsFor: 'callback UI' stamp: 'hlsf 6/19/2021 10:07:28'!
exportToBitmap: stream transparent: isTransparent
	stream ifNotNil: [ | form originalBackgroundColor labels originalLabelColor |
		World doOneCycle.
		"Before exporting to bitmap:
		paint the background as white and ensure all labels are black"
		originalBackgroundColor _ self drawable color.
		self drawable color: (isTransparent ifTrue: [Color transparent] ifFalse: [Color white]).
		form _ Form extent: self drawable extent depth: 32.
		form fillWithColor: (isTransparent ifTrue: [Color transparent] ifFalse: [Color white]).
		"repaint the labels in black as we export wiht transparent or white background"
		labels _ self drawable labels.
		labels ifNotEmpty: [originalLabelColor _ labels first color].
		labels do: [:each | each color: Color black].
		World doOneCycle.
		self drawable owner athensSurface displayOnMorphicCanvas: form getCanvas at: 0 @ 0.
		"paint back background and labels to their original color"
		self drawable color: originalBackgroundColor.
		labels do: [:each | each color: originalLabelColor].	
		World doOneCycle.
		PNGReadWriter putForm: form copy onStream: stream	]! !

