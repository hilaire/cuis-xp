parseStyleOfValue: item with: aNode 
	| costume  |
	(aNode attributeAt: #type) = 'script'
		ifTrue: [self parseStyleOfText: item with: aNode] "parse as text style"
		ifFalse: [
			costume _ app fastLastMorph.
