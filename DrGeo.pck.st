!DrGEquationLineItem methodsFor: 'accessing'!
cartesianEquation
	^ String streamContents: [:stream |
		a isZero ifFalse: [
			a = 1 ifFalse: [self printDecimal: a on: stream].
			stream	nextPutAll: 'x'].
		b isZero ifFalse: [
			a isZero ifFalse: [stream nextPutAll: ' + '].
			b = 1 ifFalse: [self printDecimal: b on: stream].
			stream	nextPutAll: 'y'].
		c isZero ifFalse: [
			stream nextPutAll: ' + '.
			self printDecimal: c on: stream].
		stream nextPutAll: ' = 0']! !

!DrGEquationLineItem methodsFor: 'accessing'!
