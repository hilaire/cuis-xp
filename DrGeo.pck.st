!DrGeoPresenter methodsFor: 'callback UI'!
askForSketchToKeep
| dialog newResource answer|
	DrGeoSystem showVirtualKeyboard: true.
	dialog _ DrGFilenameDialog new
		textFont: UITheme current textFont;
		title: 'Filename' translated;
		text: 'Name the sketch to keep.' translated.
	dialog openInWorld.
	self view openModal: dialog.
	DrGeoSystem showVirtualKeyboard: false.
	(dialog cancelled or: [dialog entryText isNil]) ifTrue: [^ nil].
	dialog network 
		ifTrue: [newResource _ DrGeoSystem networkResourcesCreateShare] 
		ifFalse: [newResource _ DrGeoSystem mySketchResources].
	newResource ifNotNil: [newResource sketchFilename: dialog entryText, '.fgeo'].
	^ newResource! !

