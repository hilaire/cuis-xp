!DrGeoDomain methodsFor: 'building'!
in: macroTree replaceParent: item with: newItem 
	| index |
	macroTree do: [ :anItem | 
		(anItem parents notNil and: [ (index _ anItem parents indexOf: item) ~= 0 ])
			ifTrue: [anItem parents at: index put: newItem ] ]! !

