		value _ String streamContents: [:stream |
		 stream nextPutAll: '(x ';
			nextPutAll: (circle center x > 0 ifTrue: [' - '] ifFalse: ['']).
		self printDecimal: circle center x on: stream.
		stream nextPutAll: ') + (y - '.
