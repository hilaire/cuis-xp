!DrGLocus2ptsItem methodsFor: 'accessing' stamp: 'hlsf 6/13/2021 11:26:20'!
closestPointTo: aPoint 
	| distance d m |
	m _ p at: 1.
	distance _ self squareNorm: aPoint - m.
	2 to: samplesQty do: [:n | 
		d _ self squareNorm: (p at: n) - aPoint.
		d < distance ifTrue: 
			[m _ p at: n.
			distance _ d]].
	^ m! !

