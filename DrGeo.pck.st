"Mouse move with pressed button"
	evt shiftPressed 
		ifTrue: [self tool handleShiftKeyMouseAt: localEventPosition ]
		ifFalse: [	
			((self tool handleMouseAt: localEventPosition) or: [self tool isBuilding]) 
				ifFalse: [self mouseMoveOxy: localEventPosition]
		].		
