updateSelection
	(self isTool: #move) ifTrue: [|node| 
		node _ self view tree selection lastSelectedNode.
		node isNil 
			ifTrue: [self tool morph: nil]
			ifFalse: [node item isMathItem ifTrue: [
				self tool morph: (app morphOf: node item)]]]! !

