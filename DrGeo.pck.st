!DrGeoPresenter methodsFor: 'callback UI' stamp: 'hlsf 4/21/2020 21:36:29'!
save: rsrc
"Save this resource and make it our default one"
| stream |
	[self saveWithinLimitedTime: [
		DrGeoSystem startSpinner.
		resource _ rsrc.
		"not yet saved"
		self view title: '* ', resource sketchFilename asFileReference basenameWithoutExtension capitalized.
		stream _ WriteStream on: (String new: 4000).
		DrGeoXml new 
			app: self app; 
			saveOn: stream.
		resource put: stream. 
		resource put: self app preview into: (resource previewFilenameFor: resource sketchFilename )].
	self app saved: true.
	stream close.
	rsrc close]
		on: Error 
		do: [:ex | DrGeoSystem stopSpinner.
			resource ifNotNil: [resource close].
			resource _ nil.
			stream close. "self halt."
			UIManager default abort: 'I can''t save the sketch.
Check the local resource or the server.' translated].
	DrGeoSystem stopSpinner.! !

