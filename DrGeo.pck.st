! !

!DrGIcons class methodsFor: 'form'!
iconWith: morphOrForm extent: extent inset: inset background: aColor
	| form imageForm|
	morphOrForm isMorph 
		ifTrue: [ |surface |
			surface _ AthensCairoSurface extent: extent.
			surface drawDuring: [:canvas| 
				morphOrForm position: 0@ ((extent y - morphOrForm height) // 2).
				canvas draw: morphOrForm].
			imageForm _ surface asForm] 
		ifFalse: [imageForm _ morphOrForm].
	form _ (FormCanvas extent: extent + (inset * 2)) fillColor: aColor.
	form paintImage: imageForm at: inset @ inset.
	^ form form! !

