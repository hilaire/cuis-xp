		ifNil: [ FontFamily addFamily: (TrueTypeFontFamily baseTTFontDescription: fontDescription ) ]
		ifNotNil: [ :fam |
			fontDescription emphasis = 0
				ifTrue: [fam baseTTFontDescription: fontDescription]
				ifFalse: [fam addTTFontDescription: fontDescription].
			fam clearCache ].	! !

