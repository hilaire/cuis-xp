	|children|
	mathItem _ aMathItem.
	target _ aTarget.
	mathItemIndex _ domain factory indexOf: mathItem.
	children _ domain factory directChildrenOf: mathItem.
	directChildren _ OrderedCollection new.
	children do:[:aChild |
		directChildren add: (
			Array 
				with: aChild 
				with: (aChild parents indexOf: mathItem))]! !

