!DrGeoXml methodsFor: 'parse style' stamp: 'hlsf 4/9/2021 19:25:15'!
parseStyleOfPoint: item with: aNode 
	| costume attribute |
	costume _ app fastLastMorph.
	self parseStyleOf: costume with: aNode.
	attribute _ aNode attributeAt: #thickness.
	attribute = 'Dashed'
		ifTrue: [attribute _ #small].
	attribute = 'Normal'
		ifTrue: [attribute _ #medium].
	attribute = 'Thick'
		ifTrue: [attribute _ #large].
	costume style pointSize: attribute.
	attribute _ aNode attributeAt: #style.
	(#('Rectangular' 'RectangularEmpty' ) includes: attribute)
		ifTrue: [attribute _ #square].
	attribute = 'RoundEmpty'
		ifTrue: [attribute _ #round].
	costume style shape: attribute asLowercase asSymbol! !

