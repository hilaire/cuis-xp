!DrGSegmentMorph methodsFor: 'drawing' stamp: 'hlsf 5/23/2021 13:30:05'!
drawMark1On: aCanvas 
	|center vector u n startPoint endPoint |
	startPoint _ vertices at: 1.
	endPoint _ vertices at: 2.
	(startPoint squaredDistanceTo: endPoint) < style borderWidth squared ifTrue: [^ self].
	center _ startPoint + endPoint / 2.0.
	vector _ endPoint - startPoint.
	[u _ vector normalized] on: ZeroDivide do: [u _ 0@1].
	n _ u y @ u x negated.
	vector _  (u + n ) normalized * (style borderWidth + 1 * 3).
	aCanvas 
		line: center - vector
		to: center + vector
		width: style borderWidth  
		color: borderColor.! !

!DrGSegmentMorph methodsFor: 'drawing' stamp: 'hlsf 5/23/2021 13:29:49'!
drawMark2On: aCanvas 
	|center vector u n startPoint endPoint |
	startPoint _ vertices at: 1.
	endPoint _ vertices at: 2.
	(startPoint squaredDistanceTo: endPoint) < (style borderWidth * 4) squared ifTrue: [^ self].
	vector _ endPoint - startPoint.
	[u _ vector normalized] on: ZeroDivide do: [u _ 0@1].
	n _ u y @ u x negated.
	vector _  (u + n ) normalized * (style borderWidth + 1 * 3).
	center _ (startPoint + endPoint / 2) - (u * (style borderWidth * 1.5)).
	2 timesRepeat: [
		aCanvas line: center - vector to: center + vector width: style borderWidth color: borderColor.
		center _ center + (u * (style borderWidth  * 3))].! !

!DrGSegmentMorph methodsFor: 'drawing' stamp: 'hlsf 5/23/2021 13:30:18'!
drawMark3On: aCanvas 
